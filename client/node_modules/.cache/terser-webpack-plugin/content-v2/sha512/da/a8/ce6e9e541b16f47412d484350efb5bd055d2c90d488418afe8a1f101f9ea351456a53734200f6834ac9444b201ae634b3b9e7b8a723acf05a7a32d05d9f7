Launch,\n    abortLaunch,\n  } = useLaunches(onSuccessSound, onAbortSound, onFailureSound);\n//HOOKS\n  const planets = usePlanets();\n  \n  return <div className={classes.content}>\n    <Header onNav={animateFrame} />\n    <Centered className={classes.centered}>\n      <Frame animate \n        show={frameVisible} \n        corners={4} \n        style={{visibility: frameVisible ? \"visible\" : \"hidden\"}}>\n        {anim => (\n          <div style={{padding: \"20px\"}}>\n          <Switch>\n            <Route exact path=\"/\">\n              <Launch \n                entered={anim.entered}\n                planets={planets}\n                submitLaunch={submitLaunch}\n                isPendingLaunch={isPendingLaunch} />\n            </Route>\n            <Route exact path=\"/launch\">\n              <Launch\n                entered={anim.entered}\n                planets={planets}\n                submitLaunch={submitLaunch}\n                isPendingLaunch={isPendingLaunch} />\n            </Route>\n            <Route exact path=\"/upcoming\">\n              <Upcoming\n                entered={anim.entered}\n                launches={launches}\n                abortLaunch={abortLaunch} />\n            </Route>\n            <Route exact path=\"/history\">\n              <History entered={anim.entered} launches={launches} />\n            </Route>\n          </Switch>\n          </div>\n        )}\n      </Frame>\n    </Centered>\n    <Footer />\n  </div>;\n};\n\nexport default withSounds()(withStyles(styles)(AppLayout));","const resources = {\n  background: {\n    small: \"/img/background-small.jpg\",\n    medium: \"/img/background-medium.jpg\",\n    large: \"/img/background-large.jpg\",\n  },\n  pattern: \"/img/glow.png\",\n};\n\nconst sounds = {\n  shared: {\n    volume: 0.5,\n  },\n  players: {\n    click: {\n      sound: { src: [\"/sound/click.mp3\"] },\n      settings: { oneAtATime: true }\n    },\n    typing: {\n      sound: { src: [\"/sound/typing.mp3\"] },\